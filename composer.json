{
    "name": "davispeixoto/laravel-test-generator",
    "description": "Laravel 4 tests generator",
    "keywords": ["laravel", "generator", "artisan", "test"],
    "license" : "MIT",
    "authors": [
        {
            "name": "Davis Peixoto",
            "email": "davis.peixoto@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "4.*",
        "illuminate/console": "4.*",
        "illuminate/filesystem": "4.*",
        "symfony/yaml": "2.5.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    },
    "autoload": {
        "classmap": [
            "src/Davispeixoto/TestGenerator",
            "tests"
        ],
        "psr-0": {
            "Davispeixoto\\TestGenerator\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
